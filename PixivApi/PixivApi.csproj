<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net8.0;net9.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Version>0.1.4</Version>
		<RootNamespace>Scighost.PixivApi</RootNamespace>
		<AssemblyName>Scighost.PixivApi</AssemblyName>
		<Authors>Scighost</Authors>
		<PackageId>Scighost.PixivApi</PackageId>
		<Description>Pixiv Web Api</Description>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<PackageProjectUrl>https://github.com/Scighost/PixivApi</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Scighost/PixivApi</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageTags>Pixiv</PackageTags>
	
		<AnalysisMode>Recommended</AnalysisMode>
		<AotCompatible>true</AotCompatible>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="NetEscapades.EnumGenerators" Version="1.0.0-beta12" />
	</ItemGroup>

</Project>
